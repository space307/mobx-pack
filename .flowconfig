[version]
0.90.0


# development files
.*/__test__/.*

[include]
# include node_modules for get typing of vendor libs
../../node_modules/

[libs]
# (github)[https://github.com/flowtype/flow-typed]
flow-typed

[lints]

[options]
suppress_comment= \\(.\\|\n\\)*\\$FlowIgnore
esproposal.decorators=ignore

# extensions
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.scss
module.name_mapper.extension='scss' -> 'empty/object'

# aliases
module.name_mapper='^demo' -> '<PROJECT_ROOT>/demo/'
module.name_mapper='^sources.js' -> '<PROJECT_ROOT>/sources.js'
module.name_mapper='^src' -> '<PROJECT_ROOT>/src/'
