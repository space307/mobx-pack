{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "newLine": "lf",
    "jsx": "react",
    "declaration": true,
    "strict": true,
    "skipLibCheck": true,
    "importHelpers": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": [
      "dom",
      "es2020"
    ]
  }
}
