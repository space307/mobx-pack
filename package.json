{
  "name": "mobx-pack",
  "version": "1.1.10",
  "description": "Library for creating application based on Mobx",
  "main": "index.js",
  "author": "Space307",
  "license": "MIT",
  "scripts": {
    "build": "babel src/lib/ --out-dir lib --copy-files",
    "prepublish": "npm run build",
    "test": "jest --verbose true",
    "test:watch": "jest --verbose true --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/space307/mobx-pack.git"
  },
  "homepage": "https://github.com/space307/mobx-pack/",
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "events": "^3.0.0",
    "lodash": "4.x",
    "mobx": "4.x",
    "mobx-react": "5.x",
    "object-assign": "^4.1.1",
    "prettier": "^1.15.3",
    "promise": "^8.0.1",
    "prop-types": "15.x",
    "react": "16.x",
    "react-dom": "16.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-decorators": "^7.2.0",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^24.5.0",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.2.1",
    "flow-bin": "0.90.0",
    "flow-typed": "^2.4.0",
    "jasmine": "^3.3.1",
    "jest": "^24.5.0",
    "jest-simple-dot-reporter": "^1.0.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-flow"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-json-strings",
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-logical-assignment-operators",
      "@babel/plugin-proposal-optional-chaining",
      [
        "@babel/plugin-proposal-pipeline-operator",
        {
          "proposal": "minimal"
        }
      ],
      "@babel/plugin-proposal-nullish-coalescing-operator",
      "@babel/plugin-proposal-do-expressions",
      "@babel/plugin-transform-runtime"
    ]
  },
  "jest": {
    "modulePaths": [
      "src"
    ],
    "modulePathIgnorePatterns": [
      "src/demo"
    ],
    "roots": [
      "src"
    ],
    "reporters": [
      "jest-simple-dot-reporter"
    ],
    "testRegex": "(\\.(test|spec)).(js|jsx|json|node)$"
  }
}
